{
  "name": "mapssearch-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend personalizado para Google Maps Scraper Dashboard",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js --watch src --ext js,json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "setup": "npm install && node scripts/setup-database.js",
    "clean": "rm -rf logs/* temp/* && npm run lint:fix",
    "optimize": "npm run lint:fix && npm run test && npm run clean",
    "production": "NODE_ENV=production node src/server.js",
    "analyze": "npm audit && npm outdated",
    "backup": "node scripts/backup-database.js",
    "migrate": "node scripts/migrate-database.js"
  },
  "keywords": [
    "google-maps",
    "scraper",
    "dashboard",
    "api",
    "express",
    "sqlite"
  ],
  "author": "Dashboard Personalizado",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.6",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "chokidar": "^3.5.3",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^9.34.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}